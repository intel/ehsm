# host ip
HOST_IP=1.2.3.4 # you must be adjust 1.2.3.4 to your host IP.

# CouchDB 
COUCHDB_PORT=5984
COUCHDB_USER=admin
COUCHDB_PASSWORD=password
COUCHDB_DB=ehsm_kms_db


# open-ssl key config setting
OPENSSL_COUNTRYNAME="CN"
OPENSSL_LOCALITYNAME="SH"
OPENSSL_ORGANIZATIONNAME="Intel"
OPENSSL_ORGANIZATIONALUNITNAME="Dev"
OPENSSL_COMMONNAME="ehsm"
OPENSSL_EMAILADDRESS="ehsm@intel.com"
# custom open-ssl key
# The default value is "", The system will automatically create a self signed certificate by open-ssl of ConfigMap
# If you want to use your own certificate, you can base 64 the contents of the certificate and put them in [tls.key] and [tls.crt]
# eg. openssl_key: ""
# eg. openssl_crt: ""
OPENSSL_KEY=""
OPENSSL_CRT=""

# docker image name
IMAGE_NAME_EHSM_BASE=ehsm_base

# eHSM version
EHSM_VERSION_STR=main # you can change the main branch to the specific tag whit you want.

# pccs url
PCCS_URL=https://1.2.3.4:8081  # you must be adjust 1.2.3.4 to your pccs server url.

# Domain key server port
DKEYSERVER_PORT=8888

# domain key cache socket localtion
DKEYCACHE_SOCKET_LOCALTION='/var/run/ehsm'
# The role of dkeyserver has two types: root and worker
#   root:  the dkeyserver will be deployed as root, and domainkey only generated in this node. 
#   worker: the dkeyserver will be deployed as worker, and domainkey will be retrieved from the root.
DKEYSERVER_ROLE="root"

# eHMS KMS port
KMS_PORT=9000
TAG_VERSION=main
